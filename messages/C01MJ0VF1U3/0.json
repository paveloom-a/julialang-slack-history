, 5],\n        :K5 =&gt; [300.0, 500, 550, 600, 400],\n        :K6 =&gt; [10.0, 8, 12, 9, 9]\n    )\n    functionlist = [minimum, maximum, minimum, maximum, maximum, minimum]\n\n\n\n    w = [0.05, 0.20, 0.10, 0.15, 0.10, 0.40]\n\n    result = grey(df, w, fs=functionlist)\n\n    @test isa(result, GreyResult)\n\n    @test isapprox(result.scores, [0.525, 0.7007142857142857,\n    0.5464285714285715, 0.5762820512820512, 0.650952380952381], atol=tol)\n\n    @test result.bestIndex == 2","user":"U01BMHAHRAM","ts":"1613042092.007400","team":"T68168MUP","blocks":[{"type":"rich_text","block_id":"Xvxa","elements":[{"type":"rich_text_section","elements":[{"type":"text","text":"tol = 0.0001\n\n    df = DataFrame(\n        :K1 => [105000.0, 120000, 150000, 115000, 135000],\n        :K2 => [105.0, 110, 120, 105, 115],\n        :K3 => [10.0, 15, 12, 20, 15],\n        :K4 => [4.0, 4, 3, 4, 5],\n        :K5 => [300.0, 500, 550, 600, 400],\n        :K6 => [10.0, 8, 12, 9, 9]\n    )\n    functionlist = [minimum, maximum, minimum, maximum, maximum, minimum]\n\n\n\n    w = [0.05, 0.20, 0.10, 0.15, 0.10, 0.40]\n\n    result = grey(df, w, fs=functionlist)\n\n    @test isa(result, GreyResult)\n\n    @test isapprox(result.scores, [0.525, 0.7007142857142857,\n    0.5464285714285715, 0.5762820512820512, 0.650952380952381], atol=tol)\n\n    @test result.bestIndex == 2"}]}]}]},{"client_msg_id":"f1f97b8e-cf43-40d2-bbe4-9847a90a85b1","type":"message","text":":innocent:","user":"U01BMHAHRAM","ts":"1613042564.007600","team":"T68168MUP","blocks":[{"type":"rich_text","block_id":"TK1P","elements":[{"type":"rich_text_section","elements":[{"type":"emoji","name":"innocent"}]}]}]}]}